{
  "name": "speakscore-api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "test": "vitest run",
    "db:migrate:public": "ts-node src/migrations/cli.ts public",
    "db:migrate:tenant": "ts-node src/migrations/cli.ts tenant",
    "db:migrate:all-tenants": "ts-node src/migrations/cli.ts all-tenants",
    "db:seed": "ts-node src/seeds/seed.ts"
  },
  "dependencies": {
    "@fastify/cors": "9.0.1",
    "@fastify/jwt": "9.2.0",
    "@fastify/sensible": "6.8.0",
    "@fastify/static": "6.12.0",
    "@fastify/multipart": "8.3.0",
    "@fastify/swagger": "8.14.1",
    "@fastify/swagger-ui": "4.0.0",
    "@speakscore/shared": "workspace:*",
    "bcryptjs": "2.4.3",
    "bullmq": "5.12.1",
    "dotenv": "16.4.5",
    "fastify": "4.26.2",
    "fastify-type-provider-zod": "2.1.1",
    "fastify-plugin": "4.5.0",
    "json2csv": "6.0.0-alpha.2",
    "pino": "9.2.0",
    "pino-pretty": "11.0.0",
    "zod": "3.23.8",
    "@aws-sdk/client-s3": "3.567.0",
    "@aws-sdk/s3-request-presigner": "3.567.0",
    "uuid": "9.0.1",
    "date-fns": "3.6.0",
    "kysely": "0.27.3",
    "pg": "8.11.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.12.11",
    "@types/uuid": "9.0.6",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5",
    "vitest": "1.6.0"
  }
}
